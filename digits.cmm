int arr[10];
int getDigitNum(int n) {
    int num = 0;
    if(n == 0) {
        return 1;
    }
    if(n < 0) {
        num = 1;
    }
    while (n != 0) {
        n = n / 10;
        num = num + 1;
    }
    return num;
}
int main() {
    int digit;
    int i = 0;
    while (i < 10) {
        arr[i] = getint();
        i = i + 1;
    }
    i = 0;
    while (i < 10) {
        digit = getDigitNum(arr[i]);
        putint(digit);
        i = i + 1;
    }
    return 0;
}