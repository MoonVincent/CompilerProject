.data
    v0:
        .space 4000
    _prompt: .asciiz "Enter an integer:"
.global main
.text
read:
    li $v0, 4
    la $a0, _prompt
    syscall
    li $v0, 5
    syscall
    jr $ra
write:
    li $v0, 1
    syscall
    jr $ra
QuickSort:
    addi $sp, $sp, -8
    sw $ra, 4($sp)
    sw $fp, 0($sp)
    addi $fp, $sp, 8
    addi $sp, $sp, -8
    sw $a0, 4($sp)
    sw $a1, 0($sp)
    lw $t0, -12($fp)
    lw $t1, -16($fp)
    blt $t0, $t1, label0
    j label1
label0:
    lw $t0, -12($fp)
    addi $sp, $sp, -4
    sw $t0, -8($fp)
    lw $t0, -16($fp)
    addi $sp, $sp, -4
    sw $t0, -12($fp)
    lw $t0, -12($fp)
    li $t1, 4
    mul $t2, $t0, $t1
    la $t0, v0
    add $t1, $t0, $t2
    lw $t1, 0($t1)
    addi $sp, $sp, -4
    sw $t1, -16($fp)
label2:
    lw $t0, -8($fp)
    lw $t1, -12($fp)
    blt $t0, $t1, label3
    j label4
label3:
label5:
    lw $t0, -8($fp)
    lw $t1, -12($fp)
    blt $t0, $t1, label8
    j label7
label8:
    lw $t0, -12($fp)
    li $t1, 4
    mul $t2, $t0, $t1
    la $t0, v0
    add $t1, $t0, $t2
    lw $t1, 0($t1)
    lw $t0, -16($fp)
    bge $t1, $t0, label6
    j label7
label6:
    lw $t0, -12($fp)
    li $t1, 1
    add $t2, $t0, $t1
    sw $t2, -12($fp)
    j label5
label7:
    lw $t0, -8($fp)
    lw $t1, -12($fp)
    blt $t0, $t1, label9
    j label10
label9:
    lw $t0, -12($fp)
    li $t1, 4
    mul $t2, $t0, $t1
    la $t0, v0
    add $t1, $t0, $t2
    lw $t1, 0($t1)
    lw $t0, -8($fp)
    li $t2, 4
    mul $t3, $t0, $t2
    la $t0, v0
    add $t2, $t0, $t3
    lw $t2, 0($t2)
    move $t2, $t1
    lw $t0, -8($fp)
    li $t1, 1
    add $t2, $t0, $t1
    sw $t2, -8($fp)
label10:
label11:
    lw $t0, -8($fp)
    lw $t1, -12($fp)
    blt $t0, $t1, label14
    j label13
label14:
    lw $t0, -8($fp)
    li $t1, 4
    mul $t2, $t0, $t1
    la $t0, v0
    add $t1, $t0, $t2
    lw $t1, 0($t1)
    lw $t0, -16($fp)
    blt $t1, $t0, label12
    j label13
label12:
    lw $t0, -8($fp)
    li $t1, 1
    add $t2, $t0, $t1
    sw $t2, -8($fp)
    j label11
label13:
    lw $t0, -8($fp)
    lw $t1, -12($fp)
    blt $t0, $t1, label15
    j label16
label15:
    lw $t0, -8($fp)
    li $t1, 4
    mul $t2, $t0, $t1
    la $t0, v0
    add $t1, $t0, $t2
    lw $t1, 0($t1)
    lw $t0, -12($fp)
    li $t2, 4
    mul $t3, $t0, $t2
    la $t0, v0
    add $t2, $t0, $t3
    lw $t2, 0($t2)
    move $t2, $t1
    lw $t0, -12($fp)
    li $t1, 1
    add $t2, $t0, $t1
    sw $t2, -12($fp)
label16:
    j label2
label4:
    lw $t0, -16($fp)
    lw $t1, -8($fp)
    li $t2, 4
    mul $t3, $t1, $t2
    la $t1, v0
    add $t2, $t1, $t3
    lw $t2, 0($t2)
    move $t2, $t0
    lw $t0, -12($fp)
    lw $t1, -8($fp)
    li $t2, 1
    add $t3, $t1, $t2
    move $a0, $t3
    move $a1, $t0
    jal QuickSort
    lw $t0, -8($fp)
    li $t1, 1
    add $t2, $t0, $t1
    lw $t0, -16($fp)
    move $a0, $t0
    move $a1, $t2
    jal QuickSort
label1:
    li $t0, 0
    move $sp, $fp
    lw $ra, -4($fp)
    lw $fp, -8($fp)
    move $v0, $t0
    jr $ra
main:
    addi $sp, $sp, -8
    sw $ra, 4($sp)
    sw $fp, 0($sp)
    addi $fp, $sp, 8
    jal read
    move $t0, $v0
    addi $sp, $sp, -4
    sw $t0, -8($fp)
    li $t0, 0
    addi $sp, $sp, -4
    sw $t0, -12($fp)
    lw $t0, -12($fp)
    li $t1, 1
    add $t2, $t0, $t1
    li $t0, 4
    mul $t1, $t2, $t0
    la $t0, v0
    add $t2, $t0, $t1
    lw $t2, 0($t2)
    lw $t0, -12($fp)
    li $t1, 4
    mul $t3, $t0, $t1
    la $t0, v0
    add $t1, $t0, $t3
    lw $t1, 0($t1)
    blt $t2, $t1, label17
    j label18
label17:
    li $t0, 0
    addi $sp, $sp, -4
    sw $t0, -16($fp)
label18:
label19:
    lw $t0, -12($fp)
    lw $t1, -8($fp)
    blt $t0, $t1, label20
    j label21
label20:
    jal read
    move $t0, $v0
    lw $t1, -12($fp)
    li $t2, 4
    mul $t3, $t1, $t2
    la $t1, v0
    add $t2, $t1, $t3
    lw $t2, 0($t2)
    move $t2, $t0
    lw $t0, -12($fp)
    li $t1, 1
    add $t2, $t0, $t1
    sw $t2, -12($fp)
    j label19
label21:
    li $t0, 0
    lw $t1, -8($fp)
    li $t2, 1
    add $t3, $t1, $t2
    move $a0, $t3
    move $a1, $t0
    jal QuickSort
    li $t0, 0
    sw $t0, -12($fp)
label22:
    lw $t0, -12($fp)
    lw $t1, -8($fp)
    blt $t0, $t1, label23
    j label24
label23:
    lw $t0, -12($fp)
    li $t1, 4
    mul $t2, $t0, $t1
    la $t0, v0
    add $t1, $t0, $t2
    lw $t1, 0($t1)
    move $a0, $t1
    jal write
    j label22
label24:
    li $t0, 0
    move $sp, $fp
    lw $ra, -4($fp)
    lw $fp, -8($fp)
    move $v0, $t0
    jr $ra
